<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newrestapi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResisterUser"
            android:label="@string/title_activity_resister_user" >
        </activity>
        
        <activity
            android:name=".MainMenuActivity"
            android:label="@string/title_activity_main_menu" >
        </activity>
        
        <!-- MENU -->
        <activity
            android:name=".MenuAndroid.MenuAluno"
            android:label="@string/title_activity_menu_aluno" >
        </activity>
        
        <activity
            android:name=".MenuAndroid.MenuProfessor"
            android:label="@string/title_activity_menu_professor" >
        </activity>
        
        
        <!-- CRUD ALUNO -->
         <activity
            android:name=".CRUD.CRUDAluno.Cadastrar"
            android:label="@string/title_activity_cadastrar_aluno" >
        </activity>
        
         <activity
            android:name=".CRUD.CRUDAluno.Listar"
            android:label="@string/title_activity_listar_aluno" >
        </activity>
        
         <activity
            android:name=".CRUD.CRUDAluno.Deletar"
            android:label="@string/title_activity_deletar_aluno" >
        </activity>
        
        <!-- CRUD PROFESSOR-->
         <activity
            android:name=".CRUD.CRUDProfessor.Cadastrar"
            android:label="@string/title_activity_cadastrar_professor" >
        </activity>
        
         <activity
            android:name=".CRUD.CRUDProfessor.Listar"
            android:label="@string/title_activity_listar_professor" >
        </activity>
        
         <activity
            android:name=".CRUD.CRUDProfessor.Deletar"
            android:label="@string/title_activity_deletar_professor" >
        </activity>
    </application>

</manifest>
